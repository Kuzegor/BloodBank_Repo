    <Content Include="Images\ph.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\BloodBankLibrary\BloodBankLibrary.csproj" />
  </ItemGroup>

</Project>
